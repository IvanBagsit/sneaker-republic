{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        { 
            "src": "/public/(.*)", 
            "dest": "/public/$1" 
        },
        { 
            "handle": "filesystem" 
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}